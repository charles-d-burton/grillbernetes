type User {
  id: ID!
  email: String!
  accessToken: String
  refreshToken: String
}

input Username {
  username: String!
}

input Login {
  email: String!
  password: String
  accessToken: String
  refreshToken: String
}

input NewUser {
  email: String!
  password: String!
}

#type Query {
#  userTaken()
#}

type Mutation {
  register(input: NewUser!):Boolean!
  login(input: Login!):User!
  signOut(input: Login!):Boolean!
  userAvailable(input: Username!):Boolean!
}